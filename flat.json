{
  "name": "Airship",
  "version": "0.1.0",
  "description": "Flat package manager",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "scripts": {},
  "main": {
    "mainClass": "flatlang/airship/Airship",
    "outDirectory": "dist",
    "outFile": "airship.js",
    "source": "src/main",
    "dependencies": {
      "../Json": "local",
      "../Compiler-String-Extensions": "local"
    }
  },
  "test": {
    "mainClass": "flatlang/airship/Airship_TestSuite",
    "outDirectory": "dist",
    "outFile": "test.js",
    "source": "src",
    "dependencies": {
      "../Nest": "local"
    }
  }
}
