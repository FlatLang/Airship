{
  "name": "Airship",
  "version": "0.7.1",
  "description": "Flat package manager",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "sources": {
    "main": {
      "mainClass": "flat/airship/Airship",
      "source": "src",
      "dependencies": {
        "https://github.com/FlatLang/Datastruct": "master",
        "https://github.com/FlatLang/Regex": "master",
        "https://github.com/FlatLang/FuCli": "master",
        "https://github.com/FlatLang/Json": "master",
        "https://github.com/FlatLang/System": "master",
        "https://github.com/FlatLang/IO": "master",
        "https://github.com/FlatLang/Git": "master",
        "https://github.com/FlatLang/Logger": "master",
        "https://github.com/FlatLang/Read-Line": "master",
        "https://github.com/FlatLang/CoLorIzer": "master",
        "https://github.com/FlatLang/RegEdit": "master",
        "https://github.com/FlatLang/Path": "master",
        "https://github.com/FlatLang/Npm-Client": "master",
        "https://github.com/FlatLang/Stream": "master",
        "https://github.com/FlatLang/TerminalSize": "master",
        "https://github.com/FlatLang/Future": "master",
        "https://github.com/FlatLang/Thread": "master"
      }
    },
    "test": {
      "mainClass": "flat/airship/Airship_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "https://github.com/FlatLang/Test": "master"
      }
    }
  }
}
