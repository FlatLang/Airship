{
  "name": "Airship",
  "version": "0.10.0",
  "description": "Flat package manager",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "sources": {
    "main": {
      "mainClass": "flat/airship/Airship",
      "source": "src",
      "dependencies": {
        "https://github.com/FlatLang/Datastruct.git": "master",
        "https://github.com/FlatLang/Regex.git": "master",
        "https://github.com/FlatLang/FuCli.git": "master",
        "https://github.com/FlatLang/Json.git": "master",
        "https://github.com/FlatLang/System.git": "master",
        "https://github.com/FlatLang/IO.git": "master",
        "https://github.com/FlatLang/Git.git": "master",
        "https://github.com/FlatLang/Logger.git": "master",
        "https://github.com/FlatLang/Read-Line.git": "master",
        "https://github.com/FlatLang/CoLorIzer.git": "master",
        "https://github.com/FlatLang/RegEdit.git": "master",
        "https://github.com/FlatLang/Path.git": "master",
        "https://github.com/FlatLang/Npm-Client.git": "master",
        "https://github.com/FlatLang/Stream.git": "master",
        "https://github.com/FlatLang/TerminalSize.git": "master",
        "https://github.com/FlatLang/Future.git": "master",
        "https://github.com/FlatLang/Thread.git": "master"
      }
    },
    "test": {
      "mainClass": "flat/airship/Airship_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "https://github.com/FlatLang/Test.git": "master"
      }
    }
  }
}
