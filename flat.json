{
  "name": "Airship",
  "version": "0.4.4",
  "description": "Flat package manager",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "sources": {
    "main": {
      "mainClass": "flat/airship/Airship",
      "source": "src",
      "dependencies": {
        "git@github.com:FlatLang/Datastruct": "master",
        "git@github.com:FlatLang/Regex": "master",
        "git@github.com:FlatLang/FuCli": "master",
        "git@github.com:FlatLang/Json": "master",
        "git@github.com:FlatLang/System": "master",
        "git@github.com:FlatLang/IO": "master",
        "git@github.com:FlatLang/Git": "master",
        "git@github.com:FlatLang/Logger": "master",
        "git@github.com:FlatLang/Read-Line": "master",
        "git@github.com:FlatLang/CoLorIzer": "master",
        "git@github.com:FlatLang/RegEdit": "master",
        "git@github.com:FlatLang/Path": "master",
        "git@github.com:FlatLang/Npm-Client": "master",
        "git@github.com:FlatLang/Stream": "master",
        "git@github.com:FlatLang/TerminalSize": "master",
        "git@github.com:FlatLang/Future": "master",
        "git@github.com:FlatLang/Thread": "master"
      }
    },
    "test": {
      "mainClass": "flat/airship/Airship_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "git@github.com:FlatLang/Test": "master"
      }
    }
  }
}
